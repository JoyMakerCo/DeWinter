      OneShotSFX:
        Name: event:/One Shot SFX/door_latch
        Parameters: []
    - Text: 'This feels like the perfect place to visit with someone who cares about
        fine food, and prefers a formal setting. '
      Background: {fileID: 0}
      Character1:
        AvatarID: 
        Pose: 
        Name: 
      Character2:
