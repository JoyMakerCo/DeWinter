        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: '"<i>Oui</i>, madame" She says with a curtsey.'
      Background: {fileID: 21300000, guid: c8e90f4327bb7464e8b64c5fe7d0275e, type: 3}
      Character1:
        AvatarID: Chambermaid
        Pose: neutral
