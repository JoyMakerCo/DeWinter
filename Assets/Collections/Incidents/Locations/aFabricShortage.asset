        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: '"<i>Salut</i> Yvette! It''s always nice to see one of my favorite customers
        outside of my little shop!"'
      Background: {fileID: 0}
      Character1:
        AvatarID: FatimaShenna
