      OneShotSFX:
        Name: 
        Parameters: []
    - Text: You ears suddenly perk up at the sound of a familiar voice, and you start
        slipping through the crowd in its direction.
      Background: {fileID: 0}
      Character1:
        AvatarID: 
        Pose: 
        Name: 
      Character2:
