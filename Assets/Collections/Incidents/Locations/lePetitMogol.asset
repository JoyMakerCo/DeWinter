      AmbientSFX:
        Name: 
        Parameters: []
      OneShotSFX:
        Name: event:/One Shot SFX/Female_bark_positive
        Parameters: []
    - Text: "\u201COh\u2026 that\u2026\u201D her cheeks flush with embarrassment.
        \u201CThat\u2019s lucky incense, I light it on slow business days.\u201D
        \r"
      Background: {fileID: 0}
