      OneShotSFX:
        Name: 
        Parameters: []
    - Text: '"Alright, my hands are starting to cramp up, so I think we''re done
        for the day."'
      Background: {fileID: 0}
      Character1:
        AvatarID: ElisabethVigeeLeBrun
        Pose: neutral
