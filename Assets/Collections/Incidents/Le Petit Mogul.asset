        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: "Her next words are a quiet mumble \u201CMy mother gave it to me.\u201D"
      Background: {fileID: 0}
      Character1:
        AvatarID: 
        Pose: 
        Name: 
      Character2:
        AvatarID: FatimaShenna
        Pose: neutral
