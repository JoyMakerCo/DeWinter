        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: '"The stove in the Cafe is broken and I have a lot of thirsty customers!"'
      Background: {fileID: 0}
      Character1:
        AvatarID: MargueriteBonnet
        Pose: afraid
        Name: Marguerite
      Character2:
        AvatarID: 
