        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: '"Alright, I already have everything I need set up. Let''s begin, shall
        we?"'
      Background: {fileID: 21300000, guid: 10e71d1ac02fa3c429ed7b45ef7219dd, type: 3}
      Character1:
        AvatarID: ElisabethLeBrun
        Pose: neutral
