      Speaker: 0
      Rewards: []
      Music:
        Name: 
        Parameters: []
      AmbientSFX:
        Name: 
        Parameters: []
      OneShotSFX:
        Name: event:/One Shot SFX/Male_bark_neutral
        Parameters: []
