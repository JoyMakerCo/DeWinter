        Name: 
        Parameters: []
      AmbientSFX:
        Name: 
        Parameters: []
      OneShotSFX:
        Name: event:/One Shot SFX/Female_bark_bored
        Parameters: []
    - Text: '"And no, I didn''t put any money into wallpaper importing."'
      Background: {fileID: 0}
      Character1:
        AvatarID: HonoradeGazel
        Pose: angry
        Name: Madame Gazel
        Motion: 0
