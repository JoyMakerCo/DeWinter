      OneShotSFX:
        Name: 
        Parameters: []
    - Text: "\u201C<i>Ja</i>. The city has many doors. Some of them are more dangerous
        than others. Madame Decaux pays me to make sure they\u2019re opened, or kept
        shut.\u201D"
      Background: {fileID: 0}
      Character1:
        AvatarID: LudovicoSidotti
        Pose: neutral
        Name: Ludovico
        Motion: 0
      Character2:
        AvatarID: Hansel
