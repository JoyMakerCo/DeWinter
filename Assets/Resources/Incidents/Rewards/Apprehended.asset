      OneShotSFX:
        Name: 
        Parameters: []
    - Text: "\u201CMy name is Hansel, Father.\u201D Hansel replies gruffly. \u201CI
        am Madame Decaux\u2019s doorman.\u201D"
      Background: {fileID: 0}
      Character1:
        AvatarID: LudovicoSidotti
        Pose: neutral
        Name: Ludovico
        Motion: 0
      Character2:
        AvatarID: Hansel
        Pose: neutral
        Name: Hansel
        Motion: 0
      Speaker: 3
      Rewards: []
