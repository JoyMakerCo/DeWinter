        Name: event:/One Shot SFX/door_latch
        Parameters: []
    - Text: "\u201C...you are trying to find someone, <i>non</i>?\u201D"
      Background: {fileID: 0}
      Character1:
        AvatarID: PierreRenaudot
        Pose: happy
        Name: Pierre
      Character2:
        AvatarID: 
        Pose: 
        Name: 
      Speaker: 0
