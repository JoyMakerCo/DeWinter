        Name: 
        Parameters: []
      AmbientSFX:
        Name: 
        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: "\u201CWhere should we look?\u201D"
      Background: {fileID: 0}
      Character1:
        AvatarID: Hansel
        Pose: neutral
        Name: Hansel
      Character2:
        AvatarID: 
        Pose: 
        Name: 
      Speaker: 2
