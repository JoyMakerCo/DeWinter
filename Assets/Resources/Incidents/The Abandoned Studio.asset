      AmbientSFX:
        Name: 
        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: "Scanning the room, you try to determine what you\u2019ll investigate."
      Background: {fileID: 0}
      Character1:
        AvatarID: 
        Pose: 
        Name: 
      Character2:
        AvatarID: 
        Pose: 
