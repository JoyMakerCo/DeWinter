      OneShotSFX:
        Name: 
        Parameters: []
    - Text: "The doorman looks perplexed at you for a moment, flipping through his
        ledger. \u201CI do not see your name here, madame\u2026\u201D\r"
      Background: {fileID: 0}
      Character1:
        AvatarID: Valet
        Pose: neutral
        Name: Doorman
      Character2:
        AvatarID: 
        Pose: 
        Name: 
      Speaker: 2
      Rewards: []
      Music:
