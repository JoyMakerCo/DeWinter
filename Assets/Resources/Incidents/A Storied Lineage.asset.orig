        Parameters: []
      AmbientSFX:
        Name: 
        Parameters: []
      OneShotSFX:
        Name: event:/One Shot SFX/Male_bark_charmed
        Parameters: []
    - Text: '"What? Absolutely not! He did what he could to help me."'
      Background: {fileID: 0}
      Character1:
        AvatarID: Thomas-AlexandreDumas
        Pose: neutral
        Name: Alex
      Character2:
        AvatarID: 
        Pose: 
