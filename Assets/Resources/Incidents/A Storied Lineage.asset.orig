      Music:
        Name: 
        Parameters: []
      AmbientSFX:
        Name: 
        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: '"I''m glad you liked it!" He laughs, clinking glasses with you.'
      Background: {fileID: 0}
      Character1:
        AvatarID: Thomas-AlexandreDumas
        Pose: happy
        Name: Alex
      Character2:
        AvatarID: 
        Pose: 
        Name: 
