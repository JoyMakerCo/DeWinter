      Music:
        Name: 
        Parameters: []
      AmbientSFX:
        Name: 
        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: '"I just don''t know."'
      Background: {fileID: 0}
      Character1:
        AvatarID: Thomas-AlexandreDumas
        Pose: neutral
        Name: Alex
