        Name: 
        Parameters: []
      AmbientSFX:
        Name: 
        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: '"In the eyes of the law, I was always a nobleman, even when I was a slave."'
      Background: {fileID: 0}
      Character1:
        AvatarID: Thomas-AlexandreDumas
        Pose: neutral
        Name: Alex
