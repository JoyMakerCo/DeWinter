    - Text: "\u201CTitles don\u2019t elevate us above consequences. Now, leave my
        house and never return.\u201D"
      Background: {fileID: 0}
      Character1:
        AvatarID: MaximinDeTermes
        Pose: smirking
        Name: Maximin
      Character2:
        AvatarID: 
        Pose: 
        Name: 
      Speaker: 2
      Rewards:
      - Type: 5
        ID: Maximin
        Value: -1
      Music:
        Name: event:/Music/Prologue - Betrayal
