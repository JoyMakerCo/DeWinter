        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: "Eventually, you part ways with each other, so you can explore the rest
        of the party. \r"
      Background: {fileID: 21300000, guid: a1b02befa27b59a4c9f91afce87e11fc, type: 3}
      Character1:
        AvatarID: 
        Pose: 
        Name: 
      Character2:
