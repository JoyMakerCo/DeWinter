        Parameters: []
    - Text: The Barmaid gives you a tiny shrug, but otherwise remains silent.
      Background: {fileID: 21300000, guid: c8e90f4327bb7464e8b64c5fe7d0275e, type: 3}
      Character1:
        AvatarID: Chambermaid
        Pose: neutral
