        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: '"However, obligations are tiresome conversation. I''d rather speak of
        passion."'
      Background: {fileID: 0}
      Character1:
        AvatarID: HonoradeGazel
        Pose: neutral
        Name: Honorade
      Character2:
        AvatarID: 
        Pose: 
        Name: 
      Speaker: 2
