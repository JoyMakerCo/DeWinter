        Parameters: []
    - Text: '[Credibility Check]'
      Background: {fileID: 0}
      Character1:
        AvatarID: ElisabethVigeeLeBrun
        Pose: neutral
        Name: "\xC9lisabeth"
      Character2:
        AvatarID: 
