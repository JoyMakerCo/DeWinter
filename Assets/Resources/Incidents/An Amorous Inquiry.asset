      Music:
        Name: 
        Parameters: []
      AmbientSFX:
        Name: 
        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: '"Oh madame, you know where to find me when you''re ready!" He says with
        a happy grin before going to request another bottle from the tavern keeper.'
      Background: {fileID: 0}
      Character1:
        AvatarID: guest03a
        Pose: approval
