        Parameters: []
      OneShotSFX:
        Name: event:/One Shot SFX/Male_bark_charmed
        Parameters: []
    - Text: "\u201CBut what about you? Are you alright?\u201D"
      Background: {fileID: 0}
      Character1:
        AvatarID: LudovicoSidotti
        Pose: neutral
