        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: "\u201CHow are you feeling, right now?\u201D"
      Background: {fileID: 0}
      Character1:
        AvatarID: LudovicoSidotti
        Pose: happy
