    Nodes:
    - Text: "You wake up the next morning to a polite knock on your bedroom\u2019s
        door frame."
      Background: {fileID: 21300000, guid: f0abd5a48ddfdda4db48289ab6e6f602, type: 3}
      Character1:
        AvatarID: 
