      OneShotSFX:
        Name: 
        Parameters: []
    - Text: 'She shifts awkwardly from foot to foot.


        "Are you are alone now, perhaps you should go meet some new people. You might
        find someone who could help you find your way in the city."'
      Background: {fileID: 0}
      Character1:
        AvatarID: 
        Pose: 
        Name: 
      Character2:
