      OneShotSFX:
        Name: 
        Parameters: []
    - Text: "\u201CIn fact, I\u2019m sure that anyone who really judges you fairly
        is going to love you.\u201D"
      Background: {fileID: 21300000, guid: f0abd5a48ddfdda4db48289ab6e6f602, type: 3}
