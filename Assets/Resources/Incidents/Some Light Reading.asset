        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
    - Text: '"Open it up."'
      Background: {fileID: 0}
      Character1:
        AvatarID: LudovicoSidotti
        Pose: neutral
        Name: Ludovico
      Character2:
        AvatarID: 
        Pose: 
