      Character2:
        AvatarID: 
        Pose: 
        Name: 
      Speaker: 0
      Rewards: []
      Music:
        Name: 
        Parameters: []
      AmbientSFX:
        Name: event:/Ambient SFX/Amb_ParisStreets_outside
        Parameters: []
      OneShotSFX:
        Name: 
        Parameters: []
