      Character1:
        AvatarID: HonoradeGazel
        Pose: sad
        Name: Honorade
      Character2:
        AvatarID: 
        Pose: 
        Name: 
      Speaker: 0
      Rewards: []
      Music:
        Name: 
        Parameters: []
      AmbientSFX:
